    rdfs:range base:StateType .

base:explains a owl:ObjectProperty ;
    rdfs:label "explains" ;
    rdfs:domain base:Explanation ;
    rdfs:range base:State .

base:hasEndEvent a owl:ObjectProperty ;
    rdfs:label "has end event" ;
    rdfs:domain base:State ;
    rdfs:range base:Event .

base:hasEndEventType a owl:ObjectProperty ;
    rdfs:label "has end event type" ;
    rdfs:domain base:StateType ;
    rdfs:range base:EventType .

base:hasMitigationPlan a owl:ObjectProperty ;
    rdfs:label "has mitigation plan" ;
    rdfs:domain base:StateType ;
    rdfs:range base:MitigationPlan .

base:hasParameterBinding a owl:ObjectProperty ;
    rdfs:label "has parameter binding" ;
    rdfs:domain base:EventDetection ;
    rdfs:range base:ParameterBinding .

base:hasPlatformType a owl:ObjectProperty ;
    rdfs:label "has platform type" ;
    rdfs:domain sosa:PlatformOfInterest ;
    rdfs:range base:PlatformType .

base:hasProperty a owl:ObjectProperty ;
