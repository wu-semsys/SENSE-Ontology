PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX sense: <http://w3id.org/explainability/sense#>
PREFIX : <http://example.org/seehub#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
select *
where { 
    sense:causalRelation rdfs:comment ?Causalcomment .
    sense:temporalRelation rdfs:comment ?Temporalcomment .
    sense:topologicalRelation rdfs:comment ?Topologicalcomment .
}