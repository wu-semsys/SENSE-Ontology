    rdfs:range base:UserType .

base:considersViewAccessRights a owl:ObjectProperty ;
    rdfs:label "considers view access rights" ;
    rdfs:domain base:Explanation ;
    rdfs:range base:ViewAccessRight .

base:containsAccessRights a owl:ObjectProperty ;
    rdfs:label "contains access rights" ;
    rdfs:domain base:UserRole ;
    rdfs:range base:AccessRight .

base:containsMitigationOption a owl:ObjectProperty ;
    rdfs:label "contains mitigation option" ;
    rdfs:domain base:MitigationPlan ;
    rdfs:range base:MitigationOption .

base:dataSource a owl:DatatypeProperty ;
    rdfs:label "data source" .

base:definition a owl:DatatypeProperty ;
    rdfs:label "definition" .

base:designedFor a owl:ObjectProperty ;
    rdfs:label "designed for" ;
    rdfs:domain base:Explanation ;
    rdfs:range base:User .

base:detectedBy a owl:ObjectProperty ;
    rdfs:label "detected by" ;
