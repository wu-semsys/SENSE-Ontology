    rdfs:range base:ResultType .

base:hasStartEvent a owl:ObjectProperty ;
    rdfs:label "has start event" ;
    rdfs:domain base:State ;
    rdfs:range base:Event .

base:hasStartEventType a owl:ObjectProperty ;
    rdfs:label "has start event type" ;
    rdfs:domain base:StateType ;
    rdfs:range base:EventType .

base:hasUserRole a owl:ObjectProperty ;
    rdfs:label "has user role" ;
    rdfs:domain base:User ;
    rdfs:range base:UserRole .

base:hasUserType a owl:ObjectProperty ;
    rdfs:label "has user type" ;
    rdfs:domain base:User ;
    rdfs:range base:UserType .

base:interestedIn a owl:ObjectProperty ;
    rdfs:label "interested in" ;
    rdfs:domain base:User ;
    rdfs:range base:StateType .

base:isTriggerState a owl:DatatypeProperty ;
    rdfs:label "is trigger state" .

