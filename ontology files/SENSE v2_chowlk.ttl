@prefix base: <http://w3id.org/explainability/sense#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <https://www.w3.org/ns/ssn/> .

dc:creator a owl:AnnotationProperty .

dc:title a owl:AnnotationProperty .

qudt:hasUnit a owl:DatatypeProperty ;
    rdfs:label "has unit" .

base: a owl:Ontology ;
    dc:creator "Fajar J. Ekaputra",
        "Katrin Schreiberhuber" ;
    dc:title "SENSE Ontology" ;
    owl:versionInfo "2.0" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

base:allowsAccessTo a owl:ObjectProperty ;
    rdfs:label "allows access to" ;
    rdfs:domain base:AccessRight ;
    rdfs:range sosa:PlatformOfInterest .

base:associatedPlatformType a owl:ObjectProperty ;
    rdfs:label "associated platform type" ;
    rdfs:domain base:ResultType ;
    rdfs:range base:PlatformType .

base:associatedProperty a owl:ObjectProperty ;
    rdfs:label "associated property" ;
    rdfs:domain base:ResultType ;
    rdfs:range sosa:ObservableProperty .

base:bindsToLiteral a owl:DatatypeProperty ;
    rdfs:label "binds to literal" .

base:bindsToSensor a owl:ObjectProperty ;
    rdfs:label "binds to sensor" ;
    rdfs:domain base:ParameterBinding ;
    rdfs:range sosa:Sensor .

base:causalRelation a owl:DatatypeProperty ;
    rdfs:label "causal relation" .

base:causallyRelated a owl:ObjectProperty ;
    rdfs:label "causally related" ;
    rdfs:domain base:State ;
    rdfs:range base:State .

base:cause a owl:ObjectProperty ;
    rdfs:label "cause" ;
