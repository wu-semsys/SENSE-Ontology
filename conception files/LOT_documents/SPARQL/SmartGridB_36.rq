PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX sense: <http://w3id.org/explainability/sense#>
PREFIX : <http://example.org/seehub#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
select ?causePlatformType
where { 
    ?platformStateType sense:associatedProperty ?Property .
    :EVCharger1 sense:hasPlatformType ?platformtype .
    ?platformtype sense:hasProperty ?property .
    ?causality sense:cause ?causeStateType ;
               sense:effect ?platformStateType .
    ?causeStateType sense:associatedProperty ?causeProperty .
    ?causePlatformType sense:hasProperty ?causeProperty .
}